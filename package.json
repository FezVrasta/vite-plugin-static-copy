{
  "name": "vite-plugin-static-copy",
  "version": "0.3.0",
  "description": "rollup-plugin-copy for vite with dev server support.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts --dts --format cjs,esm --target=node14",
    "lint": "eslint --cache .",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sapphi-red/vite-plugin-static-copy.git"
  },
  "keywords": [
    "vite",
    "vite-plugin"
  ],
  "author": "sapphi-red (https://github.com/sapphi-red)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sapphi-red/vite-plugin-static-copy/issues"
  },
  "homepage": "https://github.com/sapphi-red/vite-plugin-static-copy#readme",
  "sideEffects": false,
  "peerDependencies": {
    "vite": "^2.6.14"
  },
  "devDependencies": {
    "@polka/url": "^1.0.0-next.21",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.11",
    "@types/throttle-debounce": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@typescript/lib-dom": "npm:@types/web@^0.0.53",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mrmime": "^1.0.0",
    "picocolors": "^1.0.0",
    "prettier": "^2.5.1",
    "throttle-debounce": "^3.0.1",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5",
    "vite": "^2.8.4"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.1"
  }
}
